{
	"tabWidth": 2,
	"useTabs": false,
	"singleQuote": true,
	"trailingComma": "all",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [
	  { "files": "*.svelte", "options": { "parser": "svelte" } },
	  { "files": "*.ts", "options": { "parser": "typescript" } }
	],
	"htmlWhitespaceSensitivity": "ignore",
	"bracketSameLine": true,
	"semi": false
  }
  